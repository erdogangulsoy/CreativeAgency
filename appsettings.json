{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "SendGridApiKey": "testkey",
  "Log": {
    "ApplicationLog": {
      "PathFormat": "log_.txt",
      "FileSizeLimitBytes": 2147483648,
      "RetainedFileCountLimit": 5
    },
    "AuditLog": {
      "PathFormat": "audit_.txt",
      "FileSizeLimitBytes": 2147483648,
      "RetainedFileCountLimit": 90
    },
    "MinimumLevel": "Verbose"
  }
}
